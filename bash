#!/bin/bash 

# here we creating a file for our git...
echo "FILE CREATING"
create mkdir folder 
create touch file

# git initialize the files or folders..
echo "INIT"
git init

# adding the file
echo "ADD"
git add .

# we commit
echo "COMMIT"
git commit -m "First commit"

# we pushing to the github
echo "PUSH"
git push origin master 





